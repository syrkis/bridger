#!/bin/bash

#SBATCH --job-name=testJob        # Job name
#SBATCH --output=outs/test.out      # Name of output file (%j expands to jobId)
#SBATCH --cpus-per-task=1        # Schedule one core
#SBATCH --time=01:00:00          # Run time (hh:mm:ss) - run for one hour max
#SBATCH --gres=gpu
